    <value>desiredField = "Field_3"

-- Gets data from a the functions object (from ReferenceFunctions.dll)
-- Returns in the format {Headers = string[], Data = object[][]}
input = Funcs:getData_Lua() 

index = 0
for k,v in pairs(input.Headers) do
  if v == desiredField then index = k end
end

response = {}

if index == 0 then
  response["Error"] = "Could not find field"
else
  for i = 1, #input.Data do
    item = input.Data[i][index]

    -- Gets a random modifier value
    -- Returns in the format {Response = string, Data = number}
    item = item * Funcs:getModifier_Lua(i).Data;

    table.insert(response, item)
  end 
end

return response</value>
